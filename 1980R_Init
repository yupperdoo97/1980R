.rainbowText {
  background: linear-gradient(90deg, #ff0080, #ff8c00, #40e0d0, #8a2be2, #ff0080); /* Rainbow gradient */
  -webkit-background-clip: text;                /* Apply gradient only to text */
  -webkit-text-fill-color: transparent;        /* Make text color transparent to reveal gradient */
  animation: rainbowShift 4s linear infinite;   /* Continuous left-to-right gradient shift */
}

@keyframes rainbowShift {
  0% { background-position: 0% }                /* Start of animation */
  100% { background-position: 100% }            /* End of animation */
}
